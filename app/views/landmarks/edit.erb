<form action="/landmarks/<%= @landmark.id %>" method="post">
  <input id="hidden" type="hidden" name="_method" value="patch">
  <input id="landmark[name]" type="text" name="landmark[name]" value="<%=@landmark.name%>">
  <input id="landmark[year_completed]" type="text" name="landmark[year_completed]" value="<%=@landmark.year_completed%>">
  <input id="submit" type="submit" name="submit" value="Edit Landmark">
</form>
